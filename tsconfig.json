{
    "compilerOptions": {
      "target": "esnext",
      "module": "CommonJS",
      "moduleResolution": "node",
      "baseUrl": "src",
      "outDir": "./build",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  